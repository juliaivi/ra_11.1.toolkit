{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "plugin:react/recommended",
        "airbnb"
    ],
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "ignorePatterns": ["vite.config.js"],
    "rules": {
        "jsx-a11y/click-events-have-key-events": "off",
        "max-len": "off",
        "no-param-reassign": "off"
    }
}
